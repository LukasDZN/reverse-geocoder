{
  "name": "hotel-rating-scraper",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "private": true,
  "description": "A scraper service designed to get hotel ratings, keywords and other data that does not need to be updated frequently.",
  "main": "server.js",
  "author": "Yard Venture, UAB",
  "repository": {
    "type": "git",
    "url": "https://github.com/yardventure/hotel-rating-scraper"
  },
  "engines": {
    "node": ">=18 <19"
  },
  "type": "module",
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint --ext .ts .",
    "lintQuiet": "eslint --quiet --ext .ts .",
    "lintFix": "eslint --fix --ext .ts .",
    "prettierCheck": "prettier --check .",
    "prettierWrite": "prettier --write .",
    "test": "vitest run --no-threads",
    "test:watch": "vitest --no-threads",
    "test:coverage": "vitest run --coverage",
    "script:parseExpediaSearchHtml": "NODE_ENV=development ts-node ./src/api/v1/ratings/services/scraping/expedia/utils/devTools/parseHotelSearchHtml.util.script.ts",
    "script:parseExpediaPageHtml": "NODE_ENV=development ts-node ./src/api/v1/ratings/services/scraping/expedia/utils/devTools/parseHotelPageHtml.util.script.ts",
    "script:parseGooglePageHtml": "NODE_ENV=development ts-node ./src/api/v1/ratings/services/scraping/google/utils/devTools/parseHotelPageHtml.util.script.ts",
    "script:isHotelRelevant": "NODE_ENV=development ts-node ./src/api/v1/ratings/utils/devTools/isHotelRelevant.script.ts",
    "script:uploadOoklaWifiQualityToMongodb": "NODE_ENV=development ts-node ./scripts/ooklaWifiQuality/uploadOoklaWifiQualityToMongodb.script.ts",
    "script:uploadHotelMappings": "NODE_ENV=development ts-node ./scripts/hotelMapping/uploadHotelMappings/uploadHotelMappings.script.ts",
    "dev": "NODE_ENV=development nodemon --watch \"*.ts\" --exec \"ts-node\" ./src/server.ts",
    "build": "tsc",
    "start": "NODE_ENV=production DEBUG=local-reverse-geocoder node --max-old-space-size=8192 ./dist/server.js"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.14",
    "@types/minimist": "^1.2.2",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.4.5",
    "@types/string-similarity": "^4.0.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.2",
    "morgan": "^1.10.0",
    "nodemon": "^3.0.1",
    "prettier": "^2.8.8",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "vitest": "^0.33.0"
  },
  "dependencies": {
    "axios": "^1.2.1",
    "chalk": "^5.3.0",
    "cheerio": "^1.0.0-rc.12",
    "compression": "^1.7.4",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-winston": "^4.2.0",
    "http-status-codes": "^2.2.0",
    "local-reverse-geocoder": "^0.16.5",
    "mongoose": "^7.4.1",
    "string-similarity": "^4.0.4",
    "winston": "^3.10.0",
    "winston-mongodb": "^5.1.0",
    "zod": "^3.20.2"
  }
}
